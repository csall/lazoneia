{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)\\.(svg|jpg|png|gif|ico|webp)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/agents-bot.svg",
      "dest": "/agents-bot.svg"
    },
    {
      "src": "/glow-bot.svg",
      "dest": "/glow-bot.svg"
    },
    {
      "src": "/punchy-bot.svg",
      "dest": "/punchy-bot.svg"
    },
    {
      "src": "/lingo-bot.svg",
      "dest": "/lingo-bot.svg"
    },
    {
      "src": "/pitchy-bot.svg",
      "dest": "/pitchy-bot.svg"
    },
    {
      "src": "/scribo-bot.svg",
      "dest": "/scribo-bot.svg"
    },
    {
      "src": "/reply-bot.svg",
      "dest": "/reply-bot.svg"
    },
    {
      "handle": "filesystem"
    }
  ],
  "images": {
    "sizes": [16, 32, 48, 64, 96, 128, 180, 256, 384, 512, 1024],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "**"
      }
    ]
  }
}
